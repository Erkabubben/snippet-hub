<h1>
    <span class="far fa-list-alt"></span>CRUD Snippets
</h1>
<input></input>
<br><br>
<input></input>
<button>Log In</button>
<table class="table table-hover">
  <caption>List of code snippets</caption>
  <thead class="thead-light">
    <tr>
      <th scope="col">Created</th>
      <th scope="col">Value</th>
    </tr>
  </thead>
  <tbody>
    {{#if viewData.pureNumbers.length}}
    {{#each viewData.pureNumbers}}
    <tr>
      <th scope="row">{{this.createdAt}}</th>
      <td>{{this.value}}</td>
    </tr>
    {{/each}}
    {{else}}
    <tr colspan="2">
      <td class="text-center">No pure numbers to display.</td>
    </tr>
  {{/if}}
  </tbody>
</table>
