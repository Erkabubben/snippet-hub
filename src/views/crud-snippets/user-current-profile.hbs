<h1>
  <span class="far fa-plus-square"></span> {{user.username}}'s profile {{profileID}}
</h1>

<p>WELCOME BLAAAAH!</p>

<form method="post" action="/users/new">
  {{>validationSummary}}
  <div class="form-group">
    <input class="form-control" name="username" placeholder="Enter a username" required="required" autofocus="autofocus" />
    <br>
    <input class="form-control" name="password" placeholder="Enter a password" required="required" autofocus="autofocus" />
  </div>
  <button type="submit" class="btn btn-lg btn-primary">Register</button>
</form>